{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "skipLibCheck": true,
    "lib": [ "es5", "dom", "es2015.promise" ],
    "sourceMap": true,
    "target": "es5",
    "outDir": "wwwroot/scripts"
  },
  "exclude": [
    "node_modules"
    
  ],
  "include": [
    "wwwroot/src"
  ],
  "compileOnSave": true
}